<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>OpenTok.Net.iOS</id>
    <version>2.27.1.17</version>
    <title>Xamarin bindings to the native iOS Vonage OpenTok SDK.</title>
    <authors>s.bokatuk</authors>
    <owners>s.bokatuk</owners>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <icon>images/icon.png</icon>
    <readme>README.md</readme>
    <description>MAUI iOS Xamarin bindings for Vonage OpenTok SDK for iOS. The Vonage OpenTok iOS SDK lets you use video sessions in apps you build for iPad, iPhone.</description>
    <projectUrl>https://github.com/sbokatuk/OpenTok.Net</projectUrl>
    <dependencies>
      <group targetFramework="net7.0-ios16.1" />
      <group targetFramework="net8.0-ios17.2" />
    </dependencies>
  </metadata>
   <files>
      <file src="ios/build/OpenTok.Net.iOS.targets" target="build" />
      <file src="ios/native/lib/ios/**" target="native/lib/ios" />
      <file src="ios/icon.png" target="images/" />
      <file src="ios/README.md" target="" />
      <file src="ios/README.txt" target="" />
      <file src="ios/lib/net7.0-ios16.1/**" target="lib/net7.0-ios16.1" />
	  <file src="ios/lib/net8.0-ios17.2/**" target="lib/net8.0-ios17.2" />
    </files>
</package>